<?xml version="1.0" encoding="utf-8"?>

<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <appSettings >
    <add key="Microsoft.ServiceBus.EventHubDevices" value="{eventhub name: device data}" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
    <add key="Microsoft.ServiceBus.EventHubDevicesConsumerGroup" value="{eventhub consumer group: device data}" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
    <add key="Microsoft.ServiceBus.EventHubAlerts" value="{event hub name: alerts}" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
    <add key="Microsoft.ServiceBus.EventHubAlertsConsumerGroup" value="{event hub consumer group: alerts}" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
    <add key="Microsoft.ServiceBus.ConnectionStringDevices" value="Endpoint=sb://{sb-namespace}.servicebus.windows.net/;SharedAccessKeyName={name};SharedAccessKey={key}" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
    <add key="Microsoft.ServiceBus.ConnectionStringAlerts" value="Endpoint=sb://{sb-namespace}.servicebus.windows.net/;SharedAccessKeyName={name};SharedAccessKey={key}" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
    <add key="Microsoft.Storage.ConnectionString" value="DefaultEndpointsProtocol=https;AccountName={accountname};AccountKey={key}" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
  </appSettings>
</configuration>
